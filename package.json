{
  "name": "notes",
  "version": "1.0.0",
  "description": "A web application to write notes using Markdown",
  "main": "./server/server.js",
  "scripts": {
    "now-build": "echo 'Built on Travis CI'",
    "build:server": "babel ./server -d dist",
    "build:client": "webpack -p --config webpack.prod.js --progress",
    "build": "npm run build:server && npm run build:client",
    "dev": "nodemon ./server/server.js --exec babel-node",
    "start": "node ./dist/server.js"
  },
  "keywords": [
    "notes",
    "markdown"
  ],
  "author": "Raakesh Madana",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "nodemon": "^1.18.3",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "react-hot-loader": "^4.3.3",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.16.3",
    "webpack-cli": "^2.1.5",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.3"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "connected-react-router": "^4.3.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "highlight.js": "^9.12.0",
    "history": "^4.7.2",
    "marked": "^0.4.0",
    "mongodb": "^3.1.1",
    "mongoose": "^5.2.5",
    "monk": "^6.0.6",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "whatwg-fetch": "^2.0.4"
  }
}
